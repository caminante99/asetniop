<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px">
    <Row android:keyHeight="6%p">
        <Key android:codes="2048" android:keyLabel="Esc"/>
    </Row>

    <Row android:keyHeight="12%p" android:keyWidth="12%">
        <Key android:codes="1" android:keyLabel="a" android:horizontalGap="1%p" android:keyEdgeFlags="left"/>
        <Key android:codes="2" android:keyLabel="s"/>
        <Key android:codes="4" android:keyLabel="e"/>
        <Key android:codes="8" android:keyLabel="t"/>

        <Key android:codes="16" android:keyLabel="n" android:horizontalGap="2%p"/>
        <Key android:codes="32" android:keyLabel="i" />
        <Key android:codes="64" android:keyLabel="o"  />
        <Key android:codes="128" android:keyLabel="p" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:keyHeight="6%p">
        <Key android:codes="256" android:keyIcon="@drawable/sym_keyboard_shift" android:horizontalGap="0%p"
            android:keyWidth="20%p" android:isModifier="true"
            android:isSticky="true" android:keyEdgeFlags="left"/>
       	<Key android:codes="512" android:keyIcon="@drawable/sym_keyboard_space" android:horizontalGap="1%p"
			android:keyWidth="58%p" android:isRepeatable="true"/>
       	<Key android:codes="1024" android:keyIcon="@drawable/sym_keyboard_shift" android:horizontalGap="1%p"
            android:keyWidth="20%p" android:isModifier="true"
            android:isSticky="true" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    